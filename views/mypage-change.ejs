<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->

<!-- HEAD PARTIAL -->
<%- include ./partial/head %>
<body>

    <div id="loader">
        <div class="loader-container">
            <h3 class="loader-back-text"><img src="/images/loader.gif" alt="" class="loader"></h3>
        </div>
    </div>

    <div id="wrapper">
      <!-- HEADER PARTIAL -->
      <%- include ./partial/header.ejs %>

        <section id="page-header" class="section background">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <ul class="c1 breadcrumb text-left">
                            <li><a href="#">Home</a></li>
                            <li>비밀번호 변경</li>
                        </ul>
                        <h3>비밀번호 변경</h3>
                    </div>
                </div><!-- end row -->
            </div><!-- end container -->
        </section><!-- end section -->

        <section class="section clearfix">
            <div class="container">
                <div class="row">
                    <div id="fullwidth" class="col-sm-12">
                        <!-- START CONTENT -->
                        <div class="row">
                          <!-- MYPAGE SIDEBAR PARTIAL -->
                          <%- include ./partial/mypage_sidebar.ejs %>
                            <div id="content" class="col-md-8 col-sm-12 col-xs-12">
                              <br/>
                              <h3>핸드폰 번호</h3>
                              <hr/>
                              <div class="form-group">
                                <label for="inputPassword1" class="col-sm-2 control-label">기존 번호 :</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" id="phoneNumber" placeholder="기존 핸드폰 번호" disabled="true">
                                </div>
                                <label for="inputPassword1" class="col-sm-2 control-label">새로운 번호 :</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" id="change-phoneNumber" placeholder="새로운 핸드폰 번호">
                                </div>
                                <div class="col-md-12 text-right">
                                  <a id="change-submit-phoneNumber" class="btn btn-default">변경 하기</a>
                                </div>
                              </div>
                              <h3>비밀 번호</h3>
                              <hr/>
                              <br/>
                              <div class="form-group">
                                <label for="inputPassword1" class="col-sm-2 control-label">기존 암호 :</label>
                                <div class="col-sm-10">
                                    <input type="password" class="form-control" id="change-password1" placeholder="기존 암호">
                                </div>
                                <label for="inputPassword2" class="col-sm-2 control-label">새로운 암호 :</label>
                                <div class="col-sm-10">
                                    <input type="password" class="form-control" id="change-password2" placeholder="새로운 암호">
                                </div>
                                <label for="inputPassword3" class="col-sm-2 control-label">재입력 :</label>
                                <div class="col-sm-10">
                                    <input type="password" class="form-control" id="change-password3" placeholder="재입력">
                                </div>
                              </div>
                              <div class="col-md-12 text-right">
                                <a id="change-submit" class="btn btn-default">변경 하기</a>
                              </div>
                            </div><!-- end col -->
                        </div><!-- end row -->
                        <!-- END CONTENT -->
                    </div><!-- end fullwidth -->
                </div><!-- end row -->
            </div><!-- end container -->
        </section><!-- end section -->


        <!-- COPYRIGHTS PARTIAL -->
        <%- include ./partial/copyrights.ejs %>
    </div><!-- end wrapper -->

    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery-ui.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/sidebar.js"></script>
    <script src="/js/circle.js"></script>
    <script src="/js/progress.js"></script>
    <script src="/js/jquery.prettyPhoto.js"></script>
    <script src="/js/contact.js"></script>
    <script src="/js/parallax.js"></script>
    <script src="/js/owl.carousel.js"></script>
    <script src="/js/bootstrap-select.js"></script>
    <script src="/js/custom.js"></script>

    <script src="/js/mypage/change.js"></script>
</body>
</html>
