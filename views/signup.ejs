<body>
    <!-- HEADER PARTIAL -->
    <%- include ./partial/header.ejs %>

    <section id="page-header" class="section background">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <ul class="c1 breadcrumb text-left">
                        <li><a href="#"> Home</a></li>
                        <li>회원가입</li>
                    </ul>
                    <h3>회원가입</h3>
                </div>
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->

    <section class="section clearfix">
        <div class="container">
            <div class="row">

                <div class="comments-form clearfix">
                    <h5>회원 가입</h5>
                    <hr>
                    <form class="form-horizontal">
                        <div class="alert alert-danger" role="alert">이메일 정보가 잘못 되었습니다.</div>
                        <div class="alert alert-danger" role="alert">암호가 일치하지 않습니다.</div>
                        <div class="alert alert-danger" role="alert">이미 사용중인 이메일입니다.</div>

                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-2 control-label">이메일 :</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control" id="signup_email" placeholder="Email Address">
                            </div>
                            <label for="inputName" class="col-sm-2 control-label">이름 :</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="signup_name" placeholder="Name">
                            </div>
                            <label for="inputPassword3" class="col-sm-2 control-label">암호 :</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="singup_password1" placeholder="Password">
                            </div>
                            <label for="inputPassword3" class="col-sm-2 control-label">재입력 :</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="singup_password2" placeholder="Password">
                            </div>
                            <div class="col-sm-offset-2 col-sm-10">
                                <hr>
                                <a href="index.html" type="submit" class="btn btn-primary btn-normal border-radius pull-right">회원가입</a>
                            </div>

                        </div>
                    </form>
                </div><!-- end form -->
            </div>
        </div>
    </section>


    <!-- COPYRIGHTS PARTIAL -->
    <%- include ./partial/copyrights.ejs %>

