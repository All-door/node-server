<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->

<!-- HEAD PARTIAL -->
<%- include ./partial/head %>

<body>
    <div id="loader">
        <div class="loader-container">
            <h3 class="loader-back-text"><img src="images/loader.gif" alt="" class="loader"></h3>
        </div>
    </div>

    <!-- HEADER PARTIAL -->
    <%- include ./partial/header.ejs %>

    <section id="page-header" class="section background">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <ul class="c1 breadcrumb text-left">
                        <li><a href="#"> Home</a></li>
                        <li>회원가입</li>
                    </ul>
                    <h3>회원가입</h3>
                </div>
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->

    <section class="section clearfix">
        <div class="container">
            <div class="row">

                <div class="comments-form clearfix">
                    <h5>회원 가입</h5>
                    <hr>
                    <form class="form-horizontal">
                        <div id="signup-message" class="alert alert-danger" role="alert">이메일 정보가 잘못 되었습니다.</div>
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-2 control-label">이메일 :</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control" id="signup-email" placeholder="Email Address">
                            </div>
                            <label for="inputName" class="col-sm-2 control-label">이름 :</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="signup-name" placeholder="Name">
                            </div>
                            <label for="inputPhoneNumber" class="col-sm-2 control-label">핸드폰 번호 :</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="signup-phoneNumber" placeholder="Name">
                            </div>
                            <label for="inputPassword3" class="col-sm-2 control-label">암호 :</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="signup-password1" placeholder="Password">
                            </div>
                            <label for="inputPassword3" class="col-sm-2 control-label">재입력 :</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="signup-password2" placeholder="Password">
                            </div>
                            <div class="col-sm-offset-2 col-sm-10">
                                <hr>
                                <a id="signup-button" type="submit" class="btn btn-primary btn-normal border-radius pull-right">회원가입</a>
                            </div>
                        </div>
                    </form>
                </div><!-- end form -->
            </div>
        </div>
    </section>

    <!-- COPYRIGHTS PARTIAL -->
    <%- include ./partial/copyrights.ejs %>

    <script src="js/jquery.min.js"></script>
    <script src="js/jquery-ui.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/retina.js"></script>
    <script src="js/sidebar.js"></script>
    <script src="js/circle.js"></script>
    <script src="js/progress.js"></script>
    <script src="js/jquery.prettyPhoto.js"></script>
    <script src="js/contact.js"></script>
    <script src="js/parallax.js"></script>
    <script src="js/owl.carousel.js"></script>
    <script src="js/bootstrap-select.js"></script>
    <script src="js/custom.js"></script>

    <!-- SLIDER REVOLUTION 4.x SCRIPTS  -->
    <script src="rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script src="rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script src="js/revslider.js"></script>

    <script src="js/signup/index.js"></script>
</body>
</html>
