<!-- HEAD PARTIAL -->
<%- include ./partial/head %>
<body>

<div id="loader">
    <div class="loader-container">
        <h3 class="loader-back-text"><img src="/images/loader.gif" alt="" class="loader"></h3>
    </div>
</div>

<div id="wrapper">
  <!-- HEADER PARTIAL -->
  <%- include ./partial/header.ejs %>

    <section id="page-header" class="section background">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <ul class="c1 breadcrumb text-left">
                        <li><a href="/">Home</a></li>
                        <li>공간에 대한 자세한 정보</li>
                    </ul>
                    <h3>공간에 대한 자세한 정보</h3>
                </div>
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->
    <section class="section clearfix">
        <div class="container">
            <div class="row">
                <div id="fullwidth" class="col-sm-12">

                    <!-- START CONTENT -->
                    <div class="row">
                        <div id="content" class="col-md-12 col-sm-12 col-xs-12">
                            <div class="post-wrapper">
                                <div class="post-title clearfix">
                                    <h2 id="room-name"><strong>공간의 이름</strong></h2>
                                </div><!-- end ost-title -->

                                <div class="post-meta clearfix">
                                  <span id="room-createdAt"><i class="icon-attach"></i> 2016-05-10</span>
                                  <span id="room-favorite" class="glyphicon glyphicon-heart" style="font-size:20px; color:red;"></span>
                                </div><!-- ne dpost-meta -->

                                <div class="tags clearfix">
                                  <h5> Type : </h5>
                                  <p id="room-type" class="btn btn-default btn-normal">숙박 </p>
                                  <h5>&nbsp;Tag : </h5>
                                  <p id="room-tag" class="btn btn-default btn-normal">강남역 </p>
                                </div><!-- end tags -->
                                <br/>
                                <h3><strong>공간에 대한 설명</strong></h3>
                                <div id="room-detail">
                                  <p>설명이 들어갈 공간</p>
                                </div>
                                <hr/>
                                <div>
                                  <p id="room-address" style="margin-bottom : 0px !important;"><strong>공간 주소 : </strong>충남 홍성군 홍북면 상하리 상산 324</p>
                                  <p id="room-enabletime" style="margin-bottom : 0px !important;"><strong>사용 가능 시간 : </strong> 23:00 ~ 24:00</p>
                                  <p id="room-enableday" style="margin-bottom : 0px !important;"><strong>사용 가능 요일 : </strong> 월, 화, 수, 목, 금, 토, 일</p>
                                </div>
                                <hr/>
                                <a id="room-reservation" class="btn btn-primary btn-lg border-radius">바로 예약하러 가기</a>
                                <hr/>
                                <div class="post-content clearfix">
                                  <h3><strong>공간 사진</strong></h3>
                                  <hr>
                                  <p class="alert alert-info">사진을 클릭하면 큰 화면으로 볼수 있습니다.</p>
                                  <div id="images">
                                    <div class="room_image_padding col-md-4" style="width: 370px; height: 185px; overflow: hidden; margin-bottom:10px !important;">
                                      <img class="center-block" src="/upload/btn_bg_01.png">
                                    </div>
                                    <div class="room_image_padding col-md-4" style="width: 370px; height: 185px; overflow: hidden; margin-bottom:10px !important;">
                                      <img class="center-block" src="/upload/btn_bg_01.png">
                                    </div>
                                    <div class="room_image_padding col-md-4" style="width: 370px; height: 185px; overflow: hidden; margin-bottom:10px !important;">
                                      <img class="center-block" src="/upload/btn_bg_01.png">
                                    </div>
                                    <div class="room_image_padding col-md-4" style="width: 370px; height: 185px; overflow: hidden; margin-bottom:10px !important;">
                                      <img class="center-block" src="/upload/btn_bg_01.png">
                                    </div>
                                    <div class="room_image_padding col-md-4" style="width: 370px; height: 185px; overflow: hidden; margin-bottom:10px !important;">
                                      <img class="center-block" src="/upload/btn_bg_01.png">
                                    </div>
                                    <div class="room_image_padding col-md-4" style="width: 370px; height: 185px; overflow: hidden; margin-bottom:10px !important;">
                                    </div>
                                  </div>
                                </div><!-- end post-content -->
                                <br/>
                            </div><!-- end post-wrapper -->
                        </div><!-- end col -->
                    </div><!-- end row -->
                    <!-- END CONTENT -->

                    <div class="modal fade" id="imageModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                      <div class="modal-dialog" role="document" style="width:auto !important; height:auto !important; max-height:1024px !important;">
                        <div class="modal-content">
                          <div class="modal-body" style="max-height:100% !important;">
                            <div>
                              <img class="center-block" id="room-modal-image" width="auto" height="720px">
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                </div><!-- end fullwidth -->
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->


    <!-- COPYRIGHTS PARTIAL -->
    <%- include ./partial/copyrights.ejs %>

    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery-ui.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/sidebar.js"></script>
    <script src="/js/circle.js"></script>
    <script src="/js/progress.js"></script>
    <script src="/js/jquery.prettyPhoto.js"></script>
    <script src="/js/contact.js"></script>
    <script src="/js/parallax.js"></script>
    <script src="/js/owl.carousel.js"></script>
    <script src="/js/bootstrap-select.js"></script>
    <script src="/js/custom.js"></script>

    <script src="/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script src="/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script src="/js/revslider.js"></script>

    <script src="/js/underscore-min.js"></script>
    <script src="/js/room/index.js"></script>
    <script src="/js/room/favorite.js"></script>
</body>
</html>
