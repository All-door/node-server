                                            <div class="hotel-title">
                                                <h3 id="dashboard-title"> 공간 이름 <small>Dashboard</small></h3>
                                                <hr class="left">
                                            </div>
                                            <div class="row">
                                              <div class="col-lg-12 col-md-12">
                                                <div class="col-lg-6 col-md-6" >
                                                  <div id="dashboard-device" class="panel panel-green">
                                                        <div class="panel-heading">
                                                            <div class="row">
                                                                <div class="col-xs-offset-3 col-xs-9 text-right">
                                                                    <div class="huge" id="dashboard-device-status">디바이스 상태 : Bad</div>
                                                                    <br/>
                                                                    <div id="dashboard-device-status-time">최근 동기화 시간 : 2016-06-02 15:55</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
