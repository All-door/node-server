        <div id="wrapper">
            <!-- HEADER PARTIAL -->
            <%- include ./partial/header.ejs %>
                <section id="page-header" class="section background">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-12">
                                <ul class="c1 breadcrumb text-left">
                                    <li><a href="/">Home</a></li>
                                    <li>Dashboard</li>
                                </ul>
                                <h3>Dashboard</h3>
                            </div>
                        </div>
                    </div>
                </section>

