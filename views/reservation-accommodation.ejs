<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->

<!-- HEAD PARTIAL -->
<%- include ./partial/head %>
<body>

<div id="loader">
    <div class="loader-container">
        <h3 class="loader-back-text"><img src="/images/loader.gif" alt="" class="loader"></h3>
    </div>
</div>

<div id="wrapper">
  <!-- HEADER PARTIAL -->
  <%- include ./partial/header.ejs %>

    <section id="page-header" class="section background">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <ul class="c1 breadcrumb text-left">
                        <li><a href="/">Home</a></li>
                        <li>예약하기</li>
                    </ul>
                    <h3 id="reservation-title">'공간 이름' 예약하기</h3>
                </div>
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->

    <section class="section clearfix">
        <div class="container">
            <div class="row">
                <div id="fullwidth" class="col-sm-12">
                    <!-- START CONTENT -->
                    <div class="row">
                        <div id="content" class="col-md-12 col-sm-12 col-xs-12">
                          <div class="col-md-12">
                            <h3>예약하기</h3>
                            <hr/>
                          </div>
                          <div class="col-md-12">
                            <br/>
                            <div class="col-md-12">
                              <div class="col-md-12 panel panel-default">
                                <div class="panel-heading" style="background : gray !important;">예약 정보</div>
                                <div class="panel-body">
                                  <p><strong>예약 대기 시간 : </strong> <div id="count_down" class="text-right">05:00</div></p>
                                  <p><strong>시작 날짜 : </strong> <div id="start_day" class="text-right">2016-02-13</div></p>
                                  <p><strong>종료 날짜 : </strong> <div id="end_day" class="text-right">11:00</div></p>
                                  <p><strong>총 가격 : </strong> <div id="total_price" class="text-right">1000원</div></p>
                                </div>
                              </div>
                            </div>
                            <br/>
                            <label for="inputName" class="col-sm-2 control-label"><strong>예약 제목</strong> :</label>
                            <div class="col-sm-10">
                              <input type="text" class="form-control" placeholder="예약 제목" id="reservation-name">
                            </div>
                            <hr/>
                            <label for="inputName" class="col-sm-2 control-label"><strong>암호</strong> :</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="reservation-password" placeholder="암호를 입력해주세요">
                            </div>
                           <div class="col-sm-offset-2 col-sm-10">
                                <hr>
                                <a id="reservation-submit" type="submit" class="btn btn-primary btn-normal border-radius pull-right">예약하기</a>
                           </div>
                          </div>
                        </div><!-- end col -->
                    </div><!-- end row -->
                    <!-- END CONTENT -->
                </div><!-- end fullwidth -->
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->


    <!-- COPYRIGHTS PARTIAL -->
    <%- include ./partial/copyrights.ejs %>

    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery-ui.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/sidebar.js"></script>
    <script src="/js/circle.js"></script>
    <script src="/js/progress.js"></script>
    <script src="/js/jquery.prettyPhoto.js"></script>
    <script src="/js/contact.js"></script>
    <script src="/js/parallax.js"></script>
    <script src="/js/owl.carousel.js"></script>
    <script src="/js/bootstrap-select.js"></script>
    <script src="/js/custom.js"></script>

    <script src="/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script src="/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script src="/js/revslider.js"></script>

    <script src="/js/underscore-min.js"></script>
    <script src="/js/reservation/accommodation.js"></script>
</body>
</html>
